{
    "name": "express_typescript",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "start": "ts-node-dev --poll src/index.ts",
        "dev": "npm run build && nodemon --inspect --legacy-watch dist/index.js ",
        "prod": "nodemon --legacy-watch dist/index.js",
        "build": "tsc",
        "test": "jest --coverage --forceExit",
        "fm": "prettier --single-quote --trailing-comma all --tab-width 4 --write src/**/*.{js,json,jsx,css,scss,ts,tsx}",
        "format": "lint-staged",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/**/*.{js,json,jsx,css,scss,ts,tsx}": "prettier --single-quote --trailing-comma all --tab-width 4 --write"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.1",
        "colors": "^1.4.0",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-winston": "^4.2.0",
        "helmet": "^6.0.1",
        "joi": "^17.7.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.8.0",
        "morgan": "^1.10.0",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.6.0",
        "winston": "^3.8.2",
        "winston-mongodb": "^5.1.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.2.4",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.11.12",
        "@types/request": "^2.48.8",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-cli": "^29.3.1",
        "lint-staged": "^13.1.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "request": "^2.88.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.4"
    }
}
